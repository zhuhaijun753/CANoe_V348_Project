on timer Msg_0x326_Timer
{
  output(Msg_0x326);
  setTimer(Msg_0x326_Timer,Msg_0x326_TimerCycle);
}

on timer Msg_0x361_Timer
{
  output(Msg_0x361);
  setTimer(Msg_0x361_Timer,Msg_0x361_TimerCycle);
}



on envVar Env_RF_0x326
{
  if(getValue(this))
  {
    setTimer(Msg_0x326_Timer,Msg_0x326_TimerCycle);
  }
  else
  {
    cancelTimer(Msg_0x326_Timer);
  }
  
}

