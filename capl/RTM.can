on message TPMS_Tire_0x361
{
 if(Tire_PosFlag == 1)
{
  //write("%d",counter);
  switch(counter)
  {
    case 0:  
    {
      Msg_0x361.Tire_Position.phys = 1;
      break;
    }
     case 1:  
    {
      Msg_0x361.Tire_Position.phys = 2;
      break;
    }
     case 2:  
    {
      Msg_0x361.Tire_Position.phys = 3;
      break;
    }
     case 3:  
    {
      Msg_0x361.Tire_Position.phys = 4;
      break;
    }
  }
   counter++ ;
  if(counter > 3)
  {
    counter =0 ;
  }
}
}

on envVar Env_tire_positionFlag
{
  if(getValue(this))
  {
   Tire_PosFlag=1;
  }
  else
  {
    Tire_PosFlag=0;
    counter =0 ;
  }
  
}